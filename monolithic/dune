(library
  (name fixedPoint)
  (modules FixedPoint))

(library
  (name tez)
  (modules Tez)
  (libraries fixedPoint))

(library
  (name kit)
  (modules Kit)
  (libraries fixedPoint))

(library
  (name huxian)
  (modules Huxian)
  (libraries fixedPoint kit tez))

(executable
  (name script)
  (modules Script)
  (libraries huxian kit tez))

(executable
  (name tests)
